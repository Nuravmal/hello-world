#include<iostream>
using namespace std;
int main()
{
int temp=0;
int nums[]={2,4,1,9,8,5,4};
	for(int k=1;k<=6;k++)
        {    for(int i=0;i<=7-k-1;i++)
        {
            if(nums[i]>nums[i+1])
            {
                temp=nums[i];
                nums[i]=nums[i+1];
                nums[i+1]=temp;
            }
        }
         }
for(int i=0;i<7;i++)
cout<<nums[i]<<" ";
}
