left rotation
// C++ program to rotate an array by 
// d elements 
#include <bits/stdc++.h> 
using namespace std; 
int main()
  
{int nums[]={1,2,3,4,5,6,7};
  int v[2];
 for(int i=0;i<2;i++)
 {
   v[i]=nums[i];}
// cout<<v[i];}

 
 for(int i=0;i<5;i++)
    {
      nums[i]=nums[i+2];
    }
 int p=0;
 for(int i=5;i<7;i++)
    {
      nums[i]=v[p];
 p++;}
 
  for(int i=0;i<7;i++)
    {
      cout<<nums[i];
    }
      }
